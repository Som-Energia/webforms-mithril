{
  "name": "somenergia-webforms-mithril",
  "version": "1.0.0",
  "description": "Web front end for Som Energia forms",
  "main": "index.js",
  "scripts": {
    "deploy-prod": "webpack --env.NODE_ENV=ov_production --mode production --color --progress --optimize-minimize",
    "deploy-test": "webpack --env.NODE_ENV=testing --color --progress --optimize-minimize",
    "deploy-ov-test": "webpack --env.NODE_ENV=ov_test --color --progress --optimize-minimize",
    "build": "webpack --mode development --color --progress",
    "server": "webpack serve",
    "getlegal": "./downloadlegal.sh",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Som-Energia/webforms-mithril.git"
  },
  "keywords": [],
  "author": "Som Energia SCCL",
  "license": "AGPL-3.0+",
  "bugs": {
    "url": "https://github.com/Som-Energia/webforms-mithril/issues"
  },
  "homepage": "https://github.com/Som-Energia/webforms-mithril#readme",
  "dependencies": {
    "d3": "^7.6.1",
    "font-awesome": "^4.7.0",
    "i18n4v": "^0.2.2",
    "js-yaml": "^4.1.0",
    "json-loader": "^0.5.7",
    "material-components-web": "^14.0.0",
    "material-design-icons": "^3.0.1",
    "md-date-time-picker": "^2.3.0",
    "mithril": "^2.2.2",
    "moment": "^2.29.4",
    "mousetrap": "^1.6.5",
    "mousetrap-global-bind": "^1.1.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "dsv-loader": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.1.0",
    "html-loader-jest": "^0.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jsdoc-webpack-plugin": "^0.3.0",
    "mini-css-extract-plugin": "^2.6.1",
    "sass-loader": "^13.0.2",
    "stylus-loader": "^7.0.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "yaml-loader": "^0.8.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(scss|sass|css|styl|svg)$": "identity-obj-proxy"
    },
    "globals": {
      "__JEST_TESTING__": true
    },
    "transform": {
      "\\.html$": "./node_modules/html-loader-jest"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
